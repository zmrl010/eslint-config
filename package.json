{
  "name": "@zmrl/eslint-config",
  "version": "3.1.2",
  "description": "An evolving eslint configuration for my projects",
  "main": "./index.js",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./index.js",
    "./import": "./import.js",
    "./jest": "./jest.js",
    "./jsx-a11y": "./jsx-a11y.js",
    "./react": "./react.js",
    "./testing-library": "./testing-library.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zmrl010/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "zmrl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zmrl010/eslint-config/issues"
  },
  "homepage": "https://github.com/zmrl010/eslint-config#readme",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "find-new-rules": "run-p find-new-rules:*",
    "find-new-rules:jest": "eslint-find-rules -u ./test/jest/index.js",
    "find-new-rules:jsx-a11y": "eslint-find-rules -u \"./test/jsx-a11y/index.js\"",
    "find-new-rules:main": "eslint-find-rules -u ./index.js",
    "find-new-rules:react": "eslint-find-rules -u ./test/react/index.js",
    "lint": "eslint --config ./index.js ./src",
    "prepare": "pnpm build && (is-ci || husky install)",
    "test": "run-p lint find-new-rules",
    "build": "tsc --build"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/eslint": "^8.4.5",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.4",
    "@types/react": "^18.0.17",
    "@types/semver": "^7.3.12",
    "eslint": "^8.22.0",
    "eslint-find-rules": "^4.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.3.7"
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "pnpm-lock.yaml"
  ]
}
