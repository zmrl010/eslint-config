{
  "name": "@zmrl/eslint-config",
  "version": "1.1.2",
  "description": "A growing eslint config for projects that require it",
  "main": "./config/index.js",
  "files": [
    "patch",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zmrl010/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "zmrl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zmrl010/eslint-config/issues"
  },
  "homepage": "https://github.com/zmrl010/eslint-config#readme",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "find-new-rules": "run-p find-new-rules:*",
    "find-new-rules:jest": "eslint-find-rules -u ./test/jest/index.js",
    "find-new-rules:jsx-a11y": "eslint-find-rules -u ./test/jsx-a11y/index.js",
    "find-new-rules:main": "eslint-find-rules -u ./config/index.js",
    "find-new-rules:react": "eslint-find-rules -u ./test/react/index.js",
    "lint": "eslint --config ./config/index.js .",
    "postinstall": "is-ci || husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "run-p lint find-new-rules",
    "validate": "run-p lint"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "typescript": ">=4.6.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.16.0",
    "eslint-find-rules": "^4.1.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.3.7"
  },
  "eslintConfig": {
    "extends": "./config/index.js"
  }
}
