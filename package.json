{
  "name": "@zmrl/eslint-config",
  "version": "3.4.0",
  "description": "An evolving eslint configuration for my projects",
  "type": "module",
  "main": "./dist/configs/main.js",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": "./dist/configs/main.js",
    "./import": "./dist/configs/import.js",
    "./jest": "./dist/configs/jest.js",
    "./jsx-a11y": "./dist/configs/jsx-a11y.js",
    "./lit": "./dist/configs/lit.js",
    "./lit-a11y": "./dist/configs/lit-a11y.js",
    "./package.json": "./package.json",
    "./react": "./dist/configs/react.js",
    "./testing-library": "./dist/configs/testing-library.js",
    "./wc": "./dist/configs/wc.js"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zmrl010/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "clean code",
    "lint",
    "shareable configuration"
  ],
  "author": "zmrl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zmrl010/eslint-config/issues"
  },
  "homepage": "https://github.com/zmrl010/eslint-config#readme",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "find-new-rules": "pnpm -r --parallel find-new-rules",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint src --fix",
    "prepare": "pnpm build && (is-ci || husky install)",
    "test": "run-p lint find-new-rules",
    "build": "tsc -b",
    "build-test": "run-s build test",
    "generate:types": "generate-types"
  },
  "packageManager": "pnpm@7.15.0",
  "peerDependencies": {
    "eslint": "^8.22.0",
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/semver": "^7.3.13",
    "eslint": "^8.28.0",
    "eslint-find-rules": "^4.1.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.17.0",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "typescript": "^4.9.3",
    "generate-rule-types": "workspace:*"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@typescript-eslint/utils": "^5.44.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.3.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-wc": "^1.4.0",
    "read-pkg-up": "^9.1.0",
    "semver": "^7.3.8"
  }
}
