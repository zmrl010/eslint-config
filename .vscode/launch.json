{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node-terminal",
      "name": "Run Script: generate:types",
      "request": "launch",
      "command": "pnpm generate:types",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Debug Script: generate:types",
      "request": "launch",
      "command": "pnpm generate:types",
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_DEBUG": "generate-types"
      }
    }
  ]
}